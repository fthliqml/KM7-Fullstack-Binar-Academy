<body>
    <table>
        <thead>
            <tr>
                <th>First Name</th>
                <th>Last Name</th>
                <th>Age</th>
                <th>Phone Number</th>
                <th>Photo Profile</th>
            </tr>
        </thead>
        <tbody>
            <% users.map((user) => { %>
            <tr>
                <td><%= user.firstName %></td>
                <td><%= user.lastName %></td>
                <td><%= user.age %></td>
                <td><%= user.phoneNumber %></td>
                <td><a href="<%= user.photoProfile %>">View Profile</a></td>
            </tr>
            <%}) %>

            <!-- Using for each to looping an array -->
            <!-- <% users.forEach(user => { %>
                    <tr>
                        <td><%= user.firstName %></td>
                        <td><%= user.lastName %></td>
                        <td><%= user.age %></td>
                        <td><%= user.phoneNumber %></td>
                        <td><a href="<%= user.photoProfile %>"><%= user.photoProfile %></a></td>
                    </tr>
                <% }) %> -->
        </tbody>
    </table>
    <button onclick="location.href = 'users/create'">Add User</button>
</body>
